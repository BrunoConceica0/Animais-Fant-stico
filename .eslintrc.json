{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-param-reassign": 0,
    "import/extensions": 0,
    "import/newline-after-import": 0,
    "quotes": 0,
    "no-console": 0
  }
}
